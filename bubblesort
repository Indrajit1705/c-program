 #include<stdio.h>
int main()
{
	int arr[100],n,i,j,temp;
	printf("Enter the size of array:");
	scanf("%d",&n);
	printf("\nEnter the element of array:\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",a[i]);
	}
	for(i=0;i<n-1;i++)
    {
    	for(j=0;j<n-i-1;j++)
    	{
    		if(arr[j]>arr[j+1])
    		{
    			temp=a[j];
    			a[j]=a[j+1];
    			a[j+1]=temp;
			}
		}
	}
	printf("The sorted array is\n");
	for(i=0;i<n;i++)
	printf("%d\t",arr[i]);
}
